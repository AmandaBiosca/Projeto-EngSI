{
  "name": "projeto-eng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p start:app servJson",
    "start:app": "ng serve --port 4200",
    "servJson": "json-server mocks/data.json --routes mocks/routes.json --port 3200",
    "build": "ng build --prod",
    "lint": "ng lint",
    "copy:styles": "mkdirp dist/bv/ui/styles && cp -R projects/bv/ui/src/lib/styles dist/bv/ui",
    "copy:fonts": "mkdirp dist/bv/ui/fonts && cp -R projects/bv/ui/src/lib/fonts dist/bv/ui",
    "copy": "npm run copy:styles && npm run copy:fonts",
    "test": "jest",
    "test:libWatch": "jest --clearCache && jest --config ./jest.lib.config.js --watch",
    "test:coverage": "jest --coverage --silent",
    "test:snapshot": "jest --coverage -u",
    "test:watch": "jest --clearCache && jest --watch",
    "test:clear": "jest --clearCache",
    "test:sonar": "npm run test:coverage --force && npm run tslint:report && sonar-scanner -X",
    "e2e": "ng e2e --webdriver-update=false",
    "postinstall": "npm run build:libs:local-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "^7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
